<p>
  <strong>Chez :</strong>
  <% owners = [] %>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Personne</th>
        <th>Du</th>
        <th>Au</th>
      </tr>
    </thead>
    <tbody>
      <% item.owners.each do |owner| %>
        <tr>
          <td>
            <%= owner.user.name %>
          </td>
          <td>
            <%= l(owner.start) %>
          </td>
          <td>
            <%= l(owner.end) %>
          </td>
      <% end %>
    </tbody>
  </table>
</p>
